{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/cron/keywords",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/publish",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/cron/auto-generate",
      "schedule": "0 0,6,12,18 * * *"
    },
    {
      "path": "/api/cron/usage-reset",
      "schedule": "0 0 1 * *"
    }
  ]
}
